{
  "name": "martink/laravel-devcopilot",
  "description": "Laravel package that integrates ChatGPT into your Artisan CLI",
  "keywords": ["laravel", "chatgpt", "artisan", "gpt"],
  "license": "MIT",
  "require": {
    "php": "^8.0 || ^8.1 || ^8.2 || ^8.3 || ^8.4",
    "illuminate/support": "^12.0",
    "openai/openai": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "MartinK\\DevCopilot\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MartinK\\DevCopilot\\DevCopilotServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
